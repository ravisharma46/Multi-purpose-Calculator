<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="80mm" />
            <stroke android:width="1dip" android:color="#98f2e2" />
            <gradient android:angle="-90" android:startColor="#1ff4d1" android:endColor="#8fe2d5"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="80mm" />
            <stroke android:width="1dip" android:color="#d0fdf5" />
            <solid android:color="#58e1cb"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="80mm" />
            <stroke android:width="1dip" android:color="#0eedc4" />
            <gradient android:angle="-90" android:startColor="#c5e9e3" android:endColor="#49d9c2" />
        </shape>
    </item>
</selector>
